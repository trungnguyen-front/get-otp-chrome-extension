{
  "manifest_version": 2,
  "name": "Boilerplate extension",
  "author": "Trung Nguyen - Kenny",
  "version": "0.0.1",
  "description": "Replace new tab screen with GitHub trending projects.",
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": ["notifications", "https://*/"]
}
